<!-- Include Glide.js CSS and JS -->
<!-- Add in the <head> or before </body> -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@@glidejs/glide/dist/css/glide.core.min.css" />
<script src="https://cdn.jsdelivr.net/npm/@@glidejs/glide/dist/glide.min.js"></script>


<style>
	.glide {
		width: 100%;
		height: 100vh; /* Full viewport height */
		overflow: hidden;
	}

	.glide__slide img {
		width: 100%;
		height: 100vh;
		object-fit: cover; /* Makes images scale properly without distortion */
		display: block;
	}

	.glide__arrows button {
		background: rgba(0, 0, 0, 0.4);
		border: none;
		padding: 10px;
		border-radius: 50%;
		color: white;
		cursor: pointer;
	}

	.glide__arrows svg {
		width: 24px;
		height: 24px;
		fill: white;
	}

	/*--- Style for Clients Slider ---*/
	.slider-container {
		overflow: hidden;
		width: 100%;
		background: #fff;
		padding: 20px 0;
		white-space: nowrap;
		box-sizing: border-box;
	}

	.slider-track {
		display: inline-block;
		width: max-content;
		animation: scrollLeft 70s linear infinite;
		white-space: nowrap;
	}

		.slider-track.reverse {
			animation: scrollRight 70s linear infinite;
		}

		.slider-track img {
			height: 80px;
			margin: 0 30px;
			vertical-align: middle;
		}

	@@keyframes scrollLeft {
		0% {
			transform: translateX(0);
		}

		100% {
			transform: translateX(-50%);
		}
	}

	@@keyframes scrollRight {
		0% {
			transform: translateX(-50%);
		}

		100% {
			transform: translateX(0);
		}
	}

    /* Style For What We Do Section */
    .elementor-element.e-con-full {
        display: flex;
        gap: 30px; /* Optional: adds spacing between boxes */
        flex-wrap: nowrap; /* Ensures all boxes stay in a single row */
        justify-content: center; /* Optional: centers them horizontally */
        align-items: flex-start; /* Aligns top edges */
    }

    .elementor-widget-container {
        min-width: 250px;
        max-width: 280px;
        text-align: center;
        background-color: #f8f8f8; /* Optional: box styling */
        padding: 20px;
        border-radius: 10px;
    }

    .elementor-image-box-img img {
        display: block;
        margin: 0 auto 15px;
    }

    .elementor-image-box-title {
        font-size: 20px;
        font-weight: bold;
        margin: 10px 0 5px;
        color:royalblue;
    }

    .elementor-image-box-description {
        font-size: 17px;
        color: #555;
    }
</style>

